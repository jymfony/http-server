{
  "name": "@jymfony/http-server",
  "version": "0.1.0-dev",
  "description": "Jymfony Http Server",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "node ./tests.js"
  },
  "keywords": [
    "application",
    "http",
    "server",
    "jymfony"
  ],
  "author": "Alessandro Chitolina <alekitto@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@jymfony/autoloader": "https://github.com/jymfony/autoloader.git",
    "@jymfony/console": "https://github.com/jymfony/console.git",
    "@jymfony/contracts": "https://github.com/jymfony/contracts.git",
    "@jymfony/debug": "https://github.com/jymfony/debug.git",
    "@jymfony/event-dispatcher": "https://github.com/jymfony/event-dispatcher.git",
    "@jymfony/exceptions": "https://github.com/jymfony/exceptions.git",
    "@jymfony/http-foundation": "https://github.com/jymfony/http-foundation.git",
    "@jymfony/kernel": "https://github.com/jymfony/kernel.git",
    "@jymfony/logger": "https://github.com/jymfony/logger.git",
    "@jymfony/metadata": "https://github.com/jymfony/metadata.git",
    "@jymfony/routing": "https://github.com/jymfony/routing.git",
    "@jymfony/util": "https://github.com/jymfony/util.git"
  },
  "devDependencies": {
    "@jymfony/cache": "https://github.com/jymfony/cache.git",
    "@jymfony/debug": "https://github.com/jymfony/debug.git",
    "@jymfony/dependency-injection": "https://github.com/jymfony/dependency-injection.git",
    "@jymfony/http-client": "https://github.com/jymfony/http-client.git",
    "@jymfony/stopwatch": "https://github.com/jymfony/stopwatch.git",
    "@jymfony/testing": "https://github.com/jymfony/testing.git",
    "@jymfony/var-dumper": "https://github.com/jymfony/var-dumper.git",
    "chai": "^4.2",
    "mocha": "^10.2"
  },
  "config": {
    "jymfony-autoload": {
      "namespaces": {
        "Jymfony.Component.HttpServer": "src/"
      }
    },
    "jymfony-autoload-dev": {
      "namespaces": {
        "Jymfony.Component.HttpServer.Tests": "test/",
        "Jymfony.Component.HttpServer.Tests.Fixtures": "fixtures/"
      }
    }
  }
}
